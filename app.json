{
  "expo": {
    "name": "The Meeting House",
    "description": "When’s the last time someone asked you what you believe? You know—about the big things in life. We’re a church that loves to ask questions, and loves people with questions. We don’t have all the answers, but exploring together is pretty much our favourite thing.",
    "slug": "tmh-mobile",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    },
    "version": "6.0.2",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "splash": {
      "image": "./assets/icons/splash-black.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "scheme": "tmh",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fluidesign.mobileroadie.13460",
      "buildNumber": "4",
      "config": {
        "usesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:themeetinghouse.com",
        "applinks:www.themeetinghouse.com",
        "applinks:localhost:3006?mode=developer"
      ],
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "package": "org.tmh.takenote",
      "versionCode": 20024,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "*.themeetinghouse.com",
              "pathPrefix": "/videos"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    }
  }
}